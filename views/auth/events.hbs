<div class="events-index">
  <div class="container">
    <br>
    <div class="row header">
      <div class="col-md-4">
        <h1 style="font-weight: 100"><img src="/images/market_verde.png" style="width: 30px; height: 40px"> Mis eventos</h1>
      </div>
      <div class="col-md-8">
      </div>
    </div>
    <br>

    {{#each events}}

    <div class="container-events">

      {{!-- <div class="col-md-2">
      </div> --}}

      <div class="col-md-10 card-body">
        <h5><strong>Título del evento: </strong> {{name}}</h5>
        <p><strong>Descripción del evento: </strong>{{description}}</p>
        <p><strong>Día del evento:</strong> {{date}}</p>
        <p><strong>Dirección del evento:</strong> {{address}}</p>
        <p><strong>Asistentes:</strong></p>
        {{#each assistants}}
        <p>{{username}}</p>
        {{/each}}
        <a href="/auth/profile/events/delete?eventId={{_id}}" class="btn btn-dark">Darme de baja</a>
        <br>
      </div>
      </div>
      <hr>

      {{/each}}
  </div>